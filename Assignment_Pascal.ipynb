    "import numpy as np\n",
    "n=int(input())\n",
    "pascal = np.zeros((n,n), dtype = int)\n",
    "pascal[0,0] = pascal[1,0] = pascal [1,1] = 1\n",
    "for i in range(2,n):\n",
    "    for j in range(0,n):\n",
    "        pascal[i,j] = pascal[i-1,j-1] + pascal[i-1,j]\n",
    "print(pascal)"
